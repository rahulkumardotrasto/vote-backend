{
  "name": "vote-backend",
  "version": "0.0.0",
  "description": "vote-backend",
  "main": "server.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build && mkdir ./build/logs",
    "debug": "babel-watch ./src/server.js --presets es2015,stage-2",
    "build-server": "babel -d ./build ./src -s --presets es2015,stage-2",
    "build": "npm run clean && npm run build-server && npm run lint",
    "start": "babel-watch ./src/server.js --presets es2015,stage-2",
    "lint": "touch ./node_modules/.eslintignore && eslint ./src --fix"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bunyan": "^1.8.12",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "csvtojson": "^1.1.9",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http": "0.0.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "rand-token": "^0.4.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "safe-eval": "^0.4.1",
    "tsv": "^0.2.0",
    "xml2js": "^0.4.19",
    "xml2json": "^0.11.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0"
  },
  "engines": {
    "node": "v8.11.3"
  }
}
